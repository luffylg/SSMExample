<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

  <servlet>
    <servlet-name>FrequencyServelet</servlet-name>
    <servlet-class>com.unionpay.taskmonitor.servlet.FrequencyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FrequencyServelet</servlet-name>
    <url-pattern>/frequency</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SpiderStatusServlet</servlet-name>
    <servlet-class>com.unionpay.taskmonitor.servlet.SpiderStatusServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SpiderStatusServlet</servlet-name>
    <url-pattern>/spiderstatus</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>com.unionpay.taskmonitor.listener.PreloadListener</listener-class>
  </listener>

</web-app>
